/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "paths": {
      "lib-error-handler": [
        "dist/lib-error-handler/lib-error-handler",
        "dist/lib-error-handler"
      ],
      "ngx-design-system-wrapper": [
        "dist/ngx-design-system-wrapper/ngx-design-system-wrapper",
        "dist/ngx-design-system-wrapper"
      ],
      "ngx-nebular-wrapper": [
        "dist/ngx-nebular-wrapper/ngx-nebular-wrapper",
        "dist/ngx-nebular-wrapper"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "module": "es2020",
    "lib": ["es2020", "dom"]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true
  }
}
