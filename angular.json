{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "commudle-admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/commudle-admin",
      "sourceRoot": "projects/commudle-admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/commudle-admin",
            "index": "projects/commudle-admin/src/index.html",
            "main": "projects/commudle-admin/src/main.ts",
            "polyfills": "projects/commudle-admin/src/polyfills.ts",
            "tsConfig": "projects/commudle-admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "aot": true,
            "assets": [
              "projects/commudle-admin/src/favicon.ico",
              "projects/commudle-admin/src/assets",
              "projects/commudle-admin/src/manifest.webmanifest",
              "projects/commudle-admin/src/robots.txt",
              "projects/commudle-admin/src/sitemap.xml"
            ],
            "styles": [
              "node_modules/@fortawesome/fontawesome-svg-core/styles.css",
              "projects/commudle-admin/src/styles.scss"
            ],
            "scripts": [],
            "serviceWorker": true,
            "ngswConfigPath": "projects/commudle-admin/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "16kb",
                  "maximumError": "32kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/commudle-admin/src/environments/environment.ts",
                  "with": "projects/commudle-admin/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": true,
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "commudle-admin:build:production"
            },
            "development": {
              "browserTarget": "commudle-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "commudle-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/commudle-admin/src/test.ts",
            "polyfills": "projects/commudle-admin/src/polyfills.ts",
            "tsConfig": "projects/commudle-admin/tsconfig.spec.json",
            "karmaConfig": "projects/commudle-admin/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/commudle-admin/src/favicon.ico",
              "projects/commudle-admin/src/assets",
              "projects/commudle-admin/src/manifest.webmanifest"
            ],
            "styles": ["projects/commudle-admin/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "lib-error-handler": {
      "projectType": "library",
      "root": "projects/lib-error-handler",
      "sourceRoot": "projects/lib-error-handler/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/lib-error-handler/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/lib-error-handler/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/lib-error-handler/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lib-error-handler/src/test.ts",
            "tsConfig": "projects/lib-error-handler/tsconfig.spec.json",
            "karmaConfig": "projects/lib-error-handler/karma.conf.js"
          }
        }
      }
    },
    "ngx-design-system-wrapper": {
      "projectType": "library",
      "root": "projects/ngx-design-system-wrapper",
      "sourceRoot": "projects/ngx-design-system-wrapper/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-design-system-wrapper/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-design-system-wrapper/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-design-system-wrapper/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-design-system-wrapper/src/test.ts",
            "tsConfig": "projects/ngx-design-system-wrapper/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-design-system-wrapper/karma.conf.js"
          }
        }
      }
    },
    "ngx-nebular-wrapper": {
      "projectType": "library",
      "root": "projects/ngx-nebular-wrapper",
      "sourceRoot": "projects/ngx-nebular-wrapper/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-nebular-wrapper/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-nebular-wrapper/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-nebular-wrapper/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-nebular-wrapper/src/test.ts",
            "tsConfig": "projects/ngx-nebular-wrapper/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-nebular-wrapper/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "commudle-admin"
}
